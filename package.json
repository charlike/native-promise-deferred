{
  "name": "native-promise-deferred",
  "version": "0.1.0",
  "description": "A deferred Promise, using `native-or-another` behind and so it work on Node.js v0.10 too!",
  "repository": "tunnckoCore/native-promise-deferred",
  "author": "Charlike Mike Reagent <@tunnckoCore> (https://i.am.charlike.online)",
  "precommit.silent": true,
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "standard index.js test.js --fix --verbose",
    "test": "npm-run-all -s lint test:*",
    "test:api": "nyc --reporter lcov node test.js",
    "test:report": "nyc report",
    "prerelease": "npm test",
    "release": "standard-version --sign --no-verify",
    "git": "npm-run-all -s git:*",
    "git:add": "git add --all",
    "git:cz": "git-cz",
    "commit": "npm-run-all -s test git"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "native-or-another": "^5.0.1"
  },
  "devDependencies": {
    "commitizen": "~2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "mukla": "^0.4.9",
    "npm-run-all": "~4.0.2",
    "nyc": "^10.2.0",
    "pre-commit": "^1.2.2",
    "standard": "^9.0.0",
    "standard-version": "^4.0.0"
  },
  "files": [
    "index.js"
  ],
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [],
      "highlight": "native-promise"
    },
    "lint": {
      "reflinks": true
    }
  },
  "nyc": {
    "check-coverage": true,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0
  }
}
